plugin: vmware_vm_inventory
strict: False
hostname: ${vc_server}
username: "${vc_user}"
password: "${vc_pass}"
validate_certs: False
with_nested_properties: true
use_extra_vars: True
with_tags: True
hostnames:
- config.name
keyed_groups:
- key: tags
  prefix: "vm_tag_"
  separator: ""
filters:
- "'siwapp' in tags"
- "'${vm_name}' in config.name"